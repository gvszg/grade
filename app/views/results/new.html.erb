<% content_for :back_state do %>
  <%= link_to 'Back to State', year_student_states_path %>
<% end %>

<div class="col-md-8">
  <h3><%= @student.name %> <i class="fa fa-angle-double-right"></i> New Result</h3>
  <hr>
  <%= form_for [@year, @student, @state, @result] do |f| %>
    <div class="form-group">
      <%= f.label :subject %>
      <%= f.text_field :subject, class: "form-control", style: 'width: 50%;' %>
    </div>
    <div class="form-group">
      <%= f.label :score %>
      <%= f.text_field :score, class: "form-control", style: 'width: 50%;' %>
    </div>
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
  <% end %>
</div>